        <servlet-name>searchreponse</servlet-name>
        <url-pattern>/searchreponse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>booking</servlet-name>
        <url-pattern>/booking</url-pattern>
    </servlet-mapping>
